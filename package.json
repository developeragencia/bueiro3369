{
  "name": "bueiro-digital",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "18.17.0",
    "npm": "9.6.7"
  },
  "scripts": {
    "dev:frontend": "next dev",
    "dev:backend": "ts-node-dev --respawn --transpile-only src/server/index.ts",
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "build:frontend": "next build",
    "build:backend": "tsc -p tsconfig.server.json",
    "build": "npm run build:frontend && npm run build:backend",
    "start:frontend": "next start",
    "start:backend": "node dist/server/index.js",
    "start": "concurrently \"npm run start:frontend\" \"npm run start:backend\"",
    "lint": "next lint",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "postinstall": "prisma generate",
    "seed": "ts-node -P tsconfig.node.json src/scripts/seed.ts",
    "clean": "rm -rf .next out dist",
    "prebuild": "npm run clean && npm run prisma:generate"
  },
  "dependencies": {
